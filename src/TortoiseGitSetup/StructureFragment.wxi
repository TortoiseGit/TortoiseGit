						<RegistryKey Root="HKLM" Key="Software\[Manufacturer]" />
						<RegistryValue Root="HKLM" Key="Software\[Manufacturer]" Name="LanguageFolderPath" Type="string" Value="[INSTALLDIR]Languages" KeyPath="yes"/>
