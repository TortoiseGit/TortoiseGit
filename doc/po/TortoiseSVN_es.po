#: ../source/TortoiseGit_en.xml:4734
#: ../source/TortoiseGit_en.xml:5192
msgid "Purple"
msgstr "Púrpura"

#. (para)
#: ../source/TortoiseGit_en.xml:4669
msgid "Committing a new addition."
msgstr "Confirmando un ítem añadido."

#. (term)
#. (term)
