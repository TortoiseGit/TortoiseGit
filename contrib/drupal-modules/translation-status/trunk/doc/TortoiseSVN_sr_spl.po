#: ../source/TortoiseSVN_en.xml:12606
msgid "version extraction"
msgstr "izdvajanje verzije"

#. (primary)
#: ../source/TortoiseSVN_en.xml:12609
msgid "SubWCRev"
msgstr "SubVCRev"

#. (primary)
